<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\CommandProduct" table="command_product">
    <id name="idCp" type="integer" column="id_cp">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="refProduct" type="integer" column="ref_product" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="idClient" type="integer" column="id_client" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="commandId" type="integer" column="command_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
